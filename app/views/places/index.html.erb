<% @places.each do |place| %>
  <%= image_tag "nappie-bg.jpeg", height: 250, width: 150, crop: :fill %>

  <%# place.each do |p| %>
    <%#= cl_image_tag p.photo1.key, height: 300, width: 400, crop: :fill %>
  <%# end %>
  <%= place.name %>
  <%= place.address %>
  <strong><%= place.user.email %></strong>
  <%= link_to 'Show', place %>
  <% if policy(place).edit? %>
    <%= link_to 'Edit', edit_place_path(place) %>
  <% end %>
  <% if policy(place).destroy? %>
    <%= link_to 'Destroy', place, method: :delete, data: { confirm: 'Are you sure?' } %>
  <% end %>
  <br>
<% end %>
